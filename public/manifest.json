{
    "manifest_version": 3,
    "name": "Website Tracker",
    "version": "1.0",
    "description": "Track your website activity and visualize time spent",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "permissions": ["storage", "tabs", "alarms"],
    "background": {
      "service_worker": "src/storage.js"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }